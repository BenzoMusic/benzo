{
    "version": 2,
    "builds": [
      {
        "src": "api/telegram.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/player.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/$1.js"
      },
      {
        "src": "/(.*)",
        "dest": "public/$1"
      }
    ]
  }
  